<div class="question-block">
  <ul>

    {{#each model as |query|}}

      <li class="question-line">{{query.author}}</li>
      <h4>
        <li class="question-line">{{query.query}}</li>
        <li class="question-line">{{query.notes}}</li>
      </h4>

    {{/each}}

  </ul>
</div>
